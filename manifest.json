{
    "manifest_version": 2,
    "name": "MemeEconomy Extension",
    "description": "Manage your memes",
    "version": "0.1",
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Inject",
        "default_popup": "appmenu.html"
    },
    "content_scripts":[{
        "matches":["https://www.reddit.com/*"],
        "js": ["redditscript.js"],
        "css": ["css/redditcss.css"]
    }],
    "web_accessible_resources": [
        "data.js"
    ],
    "permissions":[
        "activeTab", "storage"
    ]
}